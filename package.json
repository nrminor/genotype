{
  "name": "genotype",
  "version": "0.1.0",
  "description": "A feature-rich TypeScript library for working with genomic data formats (FASTA, FASTQ, SAM, BAM, BED)",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/nrminor/genotype"
  },
  "engines": {
    "bun": ">=1.2.0"
  },
  "types": "src/index.ts",
  "module": "src/index.ts",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist", "src", "README.md", "LICENSE*"],
  "sideEffects": false,
  "scripts": {
    "build": "bun run build:native && bun run build:lib",
    "build:lib": "bun scripts/build.mjs --lib",
    "build:native": "bun scripts/build.mjs --native",
    "build:rust": "cargo build --release",
    "build:rust:debug": "cargo build",
    "build:with-native": "bun run build:rust && bun run build",
    "dev": "tsc --watch",
    "test": "bun test",
    "test:rust": "cargo test",
    "test:node": "node --test test/**/*.test.js",
    "lint": "tsc --noEmit && bunx biome lint",
    "lint:fix": "bunx biome lint --write",
    "lint:rust": "cargo clippy -- -D warnings",
    "format": "bunx biome format --write",
    "format:check": "bunx biome check",
    "format:rust": "cargo fmt",
    "clean": "rm -rf dist && cargo clean",
    "clean:rust": "cargo clean",
    "prepublishOnly": "bun run clean && bun run build:with-native",
    "check": "bun run lint && bun run test && bun run build",
    "check:with-native": "bun run lint && bun run test && bun run test:rust && bun run build:with-native",
    "validate": "bun run check",
    "validate:full": "bun run lint && bun run test && bun run test:rust && bun run lint:rust && bun run build:with-native",
    "docs": "typedoc",
    "docs:serve": "typedoc && bunx serve docs"
  },
  "keywords": [
    "bioinformatics",
    "genomics",
    "fasta",
    "fastq",
    "sam",
    "bam",
    "bed",
    "typescript",
    "streaming",
    "performance"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "dependencies": {
    "arktype": "^2.1.21",
    "fflate": "^0.8.2"
  },
  "optionalDependencies": {
    "genotype-darwin-x64": "0.1.0",
    "genotype-darwin-arm64": "0.1.0",
    "genotype-linux-x64": "0.1.0",
    "genotype-linux-arm64": "0.1.0",
    "genotype-win32-x64": "0.1.0",
    "genotype-win32-arm64": "0.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@types/bun": "^1.1.0",
    "@types/node": "^22.0.0",
    "tsd": "^0.31.0",
    "type-coverage": "^2.29.0",
    "typedoc": "^0.28.12",
    "typescript": "^5.5.0",
    "typescript-language-server": "^4.3.4",
    "vscode-langservers-extracted": "4.8"
  }
}
