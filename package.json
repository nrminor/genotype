{
  "name": "genotype",
  "version": "0.1.0",
  "description": "A feature-rich TypeScript library for working with genomic data formats (FASTA, FASTQ, SAM, BAM, BED)",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/nrminor/genotype"
  },
  "engines": {
    "bun": ">=1.2.0"
  },
  "types": "src/index.ts",
  "module": "src/index.ts",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE*"
  ],
  "sideEffects": false,
  "scripts": {
    "build:dev": "cd src/zig && zig build -Doptimize=Debug",
    "build:prod": "cd src/zig && zig build -Doptimize=ReleaseFast",
    "build": "bun run build:native && bun run build:lib",
    "build:lib": "bun scripts/build.mjs --lib",
    "build:native": "bun scripts/build.mjs --native",
    "build:zig": "cd src/zig && zig build",
    "build:zig:dev": "cd src/zig && zig build -Doptimize=Debug",
    "build:zig:prod": "cd src/zig && zig build -Doptimize=ReleaseFast",
    "build:with-native": "bun run build:zig:prod && bun run build",
    "dev": "tsc --watch",
    "test": "bun test",
    "test:zig": "cd src/zig && zig build test",
    "test:node": "node --test test/**/*.test.js",
    "test:deno": "deno test test/**/*.test.ts",
    "lint": "tsc --noEmit && eslint src test --ext .ts",
    "lint:fix": "eslint src test --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\" \"test/**/*.{ts,js,json}\" \"examples/**/*.{ts,js,json}\" \"*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\" \"test/**/*.{ts,js,json}\" \"*.{ts,js,json,md}\"",
    "clean": "rm -rf dist && cd src/zig && zig build clean",
    "prepublishOnly": "bun run clean && bun run build:with-native",
    "check": "bun run lint && bun run test && bun run build",
    "check:with-native": "bun run lint && bun run test && bun run test:zig && bun run build:with-native",
    "validate": "bun run check",
    "validate:full": "bun run check:with-native"
  },
  "keywords": [
    "bioinformatics",
    "genomics",
    "fasta",
    "fastq",
    "sam",
    "bam",
    "bed",
    "typescript",
    "streaming",
    "performance"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "dependencies": {
    "arktype": "^2.0.0-rc.8"
  },
  "optionalDependencies": {
    "genotype-darwin-x64": "0.1.0",
    "genotype-darwin-arm64": "0.1.0",
    "genotype-linux-x64": "0.1.0",
    "genotype-linux-arm64": "0.1.0",
    "genotype-win32-x64": "0.1.0",
    "genotype-win32-arm64": "0.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@eslint/js": "^9.32.0",
    "@types/bun": "^1.1.0",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.32.0",
    "prettier": "^3.3.0",
    "tsd": "^0.31.0",
    "type-coverage": "^2.29.0",
    "typescript": "^5.5.0",
    "typescript-language-server": "^4.3.4",
    "vscode-langservers-extracted": "4.8"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "bracketSameLine": false,
    "quoteProps": "as-needed"
  }
}
